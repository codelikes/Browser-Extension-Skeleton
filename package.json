{
  "name": "chrome-ext-qwerty-bookmarkmanager",
  "version": "0.1.0",
  "description": "My Chrome Extension",
  "private": true,
  "scripts": {
    "clear": "rm -rf build/*",
    "watch": "npm run clear && webpack --mode=development --watch --config config/webpack.config.js",
    "build": "npm run clear && webpack --mode=production --config config/webpack.config.js",
    "pack": "node pack.js",
    "repack": "npm run build && npm run pack",
    "format": "prettier --write --ignore-unknown \"{config,public,src}/**/*.{html,css,js,ts,json}\"",
    "gpt-helper:generate": "./gpt.sh -e \"js,json5,ts\" ."
  },
  "devDependencies": {
    "adm-zip": "^0.5.10",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.8.1",
    "file-loader": "^6.2.0",
    "json5": "^2.2.3",
    "mini-css-extract-plugin": "^2.7.6",
    "prettier": "^2.8.8",
    "raw-loader": "^4.0.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.9.0"
  }
}
